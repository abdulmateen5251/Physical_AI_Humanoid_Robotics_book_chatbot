# Environment template for PDCP Frontend
# Copy this file to .env.local and fill in your actual values

# API Configuration
NEXT_PUBLIC_API_URL="http://localhost:3000/api"

# Authentication
NEXT_PUBLIC_AUTH_URL="http://localhost:3000"

# Feature Flags (must match backend)
NEXT_PUBLIC_FEATURE_QUESTIONNAIRE_ENABLED="true"
NEXT_PUBLIC_FEATURE_RECOMMENDATIONS_ENABLED="false"
NEXT_PUBLIC_FEATURE_TEMPLATES_ENABLED="false"

# Analytics (optional)
NEXT_PUBLIC_ANALYTICS_ENABLED="false"

# Environment
NEXT_PUBLIC_ENV="development"
